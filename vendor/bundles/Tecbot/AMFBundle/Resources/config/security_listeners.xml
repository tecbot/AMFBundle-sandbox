<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="tecbot_amf.security.access_map" class="%security.access_map.class%" public="false"/>

        <service id="tecbot_amf.security.context_listener" class="%security.context_listener.class%" public="false">
            <argument type="service" id="security.context"/>
            <argument type="collection"></argument>
            <argument/>
            <argument type="service" id="logger" on-invalid="null"/>
        </service>

        <service id="tecbot_amf.security.access_listener" class="%security.access_listener.class%" public="false">
            <argument type="service" id="security.context"/>
            <argument type="service" id="security.access.decision_manager"/>
            <argument type="service" id="tecbot_amf.security.access_map"/>
            <argument type="service" id="security.authentication.manager"/>
            <argument type="service" id="logger" on-invalid="null"/>
        </service>

    </services>

</container>